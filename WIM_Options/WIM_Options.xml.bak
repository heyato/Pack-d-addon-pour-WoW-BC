			<OnDragStop>
				this:StopMovingOrSizing();
				this.isMoving = false;
				WIM_Data.winLoc.left = this:GetLeft();
				WIM_Data.winLoc.top = this:GetTop();
				WIM_CascadeStep = 0;
			</OnDragStop>
			<OnEnter>
				GameTooltip:SetOwner(this, "ANCHOR_RIGHT");
				GameTooltip:SetText(WIM_LOCALIZED_OPTIONS_TOOLTIP_MSG_SPAWN);
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Frame>
	
	
	
	
